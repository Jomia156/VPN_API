generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model peer {
      id Int @id @default(autoincrement())
      peerName String @unique()
      PrivateKey String
      PublicKey String
      PresharedKey String
      AllowedIPs String @unique() 
      created_date DateTime @default(now())
      shelflife DateTime
      banned Boolean @default(false)
}
